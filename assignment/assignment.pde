void setup() {
  size(500, 500); // Size of canvas
  background(200, 200, 200); // Background color as rgb values
}

void draw() {
  ellipseMode(CENTER);
  fill(255,255,255);
  stroke(0,0,0);
  strokeWeight(1.1);
  ellipse(250,250,100,100);
}
